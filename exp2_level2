#include<stdio.h>
int main() 
{
    char ch;
    scanf("%c",&ch); 
    if((ch>='a' && ch<='z')||(ch>='A' && ch<='Z'))
    {
         if(ch>='a' && ch<='z')
         {
              printf("The alphabet is lowercase");
         }
         else 
         {
              printf("The alphabet is uppercase"); 
         }
    } 
    else if(ch>='0' && ch<='9')
    { 
        printf("It is digit");
    }
    else 
    {
         printf("It is special character"); 
    }
  return 0;
 }
